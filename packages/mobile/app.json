{
  "expo": {
    "name": "KinPath",
    "slug": "kinpath",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "kinpath",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#fdf4f0"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kinpath.app"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-web-browser",
      "@react-native-community/datetimepicker"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "your-eas-project-id"
      }
    }
  }
}
